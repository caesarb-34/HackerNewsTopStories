{
  "name": "orchis-login-web",
  "version": "9.0.15-SNAPSHOT",
  "com.standard": {
    "distributionUrl": "https://nexus.standard.com:8443/nexus/content/repositories/releases",
    "groupId": "com.standard.sic",
    "artifactId": "orchis-login-web"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve-mock": "ng serve --configuration=mock-development --proxyConfig='proxy.conf.json'",
    "serve-ssl": "ng serve --ssl=true --sslKey='./ssl/cjs-demo.key' --sslCert='./ssl/cjs-demo.crt' --disableHostCheck --proxyConfig=proxy.conf.json",
    "build:timestamp": "ts-node node_modules/sfg-ng-brand-library/scripts/build-timestamp.ts",
    "build": "ng build",
    "test": "ng test --browsers=ChromeHeadless --watch=false",
    "test-debug": "ng test --watch=true",
    "test-coverage": "ng test --browsers=ChromeHeadless --codeCoverage --watch=false",
    "test-coverage-debug": "ng test --codeCoverage --watch=true --source-map=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rm -rf dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.5",
    "@angular/common": "~10.2.5",
    "@angular/compiler": "~10.2.5",
    "@angular/core": "~10.2.5",
    "@angular/forms": "~10.2.5",
    "@angular/platform-browser": "~10.2.5",
    "@angular/platform-browser-dynamic": "~10.2.5",
    "@angular/router": "~10.2.5",
    "@ng-idle/core": "^8.0.0-beta.4",
    "angular2-text-mask": "^9.0.0",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "cloudentityjs": "^1.10.1",
    "core-js": "^3.6.5",
    "detect-browser": "^5.1.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^5.6.1",
    "rxjs": "^6.5.5",
    "sfg-ng-brand-library": "^10.0.0",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "~10.2.5",
    "@angular/language-service": "~10.2.5",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "moment-timezone": "^0.5.31",
    "protractor": "~5.4.3",
    "replace-in-file": "^6.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.9.2"
  },
  "engines": {
    "node": "12.16.1",
    "npm": "6.13.4"
  }
}
